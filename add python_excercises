# Small Python exercises and tests 

num = 25
synth = f"Moog Subsequent {num}"
print(synth)

synth = f"   {synth}   "
synth = synth.strip()
print(synth)

synth = synth.replace("25", "37")
print(synth)

is_moog = "Moog" in synth
print(is_moog)


# CONDITIONALS

if (synth == "Moog Subsequent 25"):
    print("Enjoy your Moog Subsequent 25")
elif (synth == "Moog Subsequent 37"):
    print("Enjoy your Moog Subsequent 37")
else:
    print("Go buy a moog!")